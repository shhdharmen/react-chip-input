{
  "name": "react-chip-input",
  "version": "1.0.5",
  "description": "Chip input for react, based on react-bootstrap",
  "author": "shhdharmen",
  "license": "MIT",
  "repository": "shhdharmen/react-chip-input",
  "keywords": [
    "react",
    "chip-input",
    "react-bootstrap",
    "bootstrap",
    "react-chip-input"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn build",
    "deploy": "gh-pages -d example/build --user 'Dharmen Shah <shhdharmen@gmail.com>'",
    "gh-pages-help": "gh-pages --help"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react-bootstrap": "^1.0.0-beta.5"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^2.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ]
}
